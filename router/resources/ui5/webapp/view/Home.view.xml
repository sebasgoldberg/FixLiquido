<mvc:View
	controllerName="jsg.fixpo.controller.Home"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		title="{i18n>title}">
		<footer>
			<OverflowToolbar>
				<ToolbarSpacer/>
				<Button type="Accept" text="Start" press="onStart" />
				<Button type="Reject" text="Stop" press="onStop" />
			</OverflowToolbar>
		</footer>

	</Page>
</mvc:View>