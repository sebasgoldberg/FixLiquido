{
  "welcomeFile": "/web/index.html",
  "authenticationMethod": "route",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/daemon/(.*)$",
		"destination": "daemon_api",
		"authenticationType": "xsuaa"
	},
    {
      "source": "^/web/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    }
  ]
}